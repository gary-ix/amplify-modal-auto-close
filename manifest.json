{
  "manifest_version": 3,
  "name": "AWS Modal Auto Close",
  "version": "1.0",
  "description": "Automatically closes migrate modal in AWS Amplify Console",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.aws.amazon.com/*", "*://aws.amazon.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 